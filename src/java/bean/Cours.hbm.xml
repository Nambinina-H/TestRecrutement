<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bean.Cours" table="cours">
        <id name="codecours" column="codecours">
        </id>
        <property name="Titre" column="Titre"></property>
        <property name="Contenu" column="Contenu"></property>
        <property name="Categorie" column="Categorie"></property>
    </class>
</hibernate-mapping>